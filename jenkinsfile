pipeline {
  
  agent any
  
  stages {
  
    stage("build") {
      steps{
        echo "building the application . . . "
      }
    }
    
    stage("test") {
      steps{
        echo "start testing . . ."
        echo "end testing . . ."
      }
    }
  
    stage("deploy"){
      steps{
        echo "deploying . . ."
      }
    }
  }
}
